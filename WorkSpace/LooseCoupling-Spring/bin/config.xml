<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Define your beans here -->
    
     <bean id="mathCheatSheet" class="com.sk.MathCheatSheet"></bean>
    <bean id="scienceCheatSheet" class="com.sk.ScienceCheatSheet"></bean>
  
  
  
   <bean id="student" class="com.sk.Student">
 <property name="cheatSheet" ref="mathCheatSheet"></property>
   </bean>
  
    
   
</beans>





